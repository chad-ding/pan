{
	"name": "pan",
	"description": "H5开发工具集",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": ["tabs", "activeTab", "cookies"],
	"action": {
		"default_popup": "popup/index.html",
		"default_icon": {
			"128": "icon/pan.png"
		}
	},
	"icons": {
		"128": "icon/pan.png"
	},
	"background": {
		"service_worker": "background/index.js"
	},
	"host_permissions": ["<all_urls>"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content/index.js"],
			"run_at": "document_start"
		}
	]
}
